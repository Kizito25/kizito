<%- include('../partials/head.ejs') %>
<body id="talk__body ">
  <!-- Main Header  start-->
  <%- include('../partials/header.ejs') %>
  <!-- Main Header end -->
  <main class="talk__main">
    <div class="<%=classGroup%>__divider"></div>
    <div class="<%=classGroup%>__container">
      <!-- Main Profile Card -->
      <div class="<%=classGroup%>__profile_card">
        <img
          class="<%=classGroup%>__profile_card__img"
          src="img/profile-img.png"
          alt="Profile Image"
          srcset=""
          height="200"
          width="200"
        />
        <div class="<%=classGroup%>__profile_card__body">
          <h1 class="<%=classGroup%>__profile_card__name">Ihugba Kizito</h1>
          <h3 class="<%=classGroup%>__profile_card__stack">
            Backend Developer
          </h3>
        </div>
      </div>
      <div class="<%=classGroup%>__card">
        <div class="<%=classGroup%>__card_items">
          <img
            src="img/profile-img.png"
            width="260"
            height="300"
            alt=""
            srcset=""
          />
          <div class="<%=classGroup%>__details">
            <h2 class="<%=classGroup%>__title">Project One</h2>
            <p class="<%=classGroup%>__description">Description...</p>
            <p class="<%=classGroup%>__authors">Authors</p>
            <p class="<%=classGroup%>__tech">Tech</p>
          </div>
        </div>
        <div class="<%=classGroup%>__card_items item-1">
          <img
            src="img/profile-img.png"
            width="260"
            height="300"
            alt=""
            srcset=""
          />
          <div class="<%=classGroup%>__details">
            <h2 class="<%=classGroup%>__title">Project One</h2>
            <p class="<%=classGroup%>__description">Description...</p>
            <p class="<%=classGroup%>__authors">Authors</p>
            <p class="<%=classGroup%>__tech">Tech</p>
          </div>
        </div>
      </div>
    </div>
    <!-- Main Profile Card end -->
    <div class="<%=classGroup%>__profile-footer">
      <!-- Social Icons  start-->
      <div class="<%=classGroup%>__profile-footer_social">
        <ul>
          <li><i class="fab fa-facebook"></i><span> Facebook</span></li>
          <li><i class="fab fa-linkedin"></i><span> LinkedIn</span></li>
          <li><i class="fab fa-github"></i><span> GitHub</span></li>
          <li><i class="fab fa-twitter"></i><span> Twitter</span></li>
        </ul>
      </div>
      <!-- Social Icons end -->
      <!-- Contact Icon start -->
      <div id="main__profile_contact" class="main_profile_contact">
        <%- include('../layouts/messenger.ejs') %>
      </div>
    </div>
  </main>
</body>
<%- include('../partials/tail.ejs') %>
